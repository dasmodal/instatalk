<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-8">
    <h1>Rooms</h1>

    <ul class="mt-4 list-unstyled">
      <%= render @rooms %>
    </ul>

    <div class="mt-2">
      <%= render 'rooms/form', room: @room %>
    </div>
  </div>

  <div class="col-4">
    <h2>Online:</h2>

    <ul class="mt-4 list-unstyled" id="presence_users">
      <%= render @users %>
    </ul>
  </div>
</div>
